{
  "name": "angularjs-webpack-boilerplate",
  "description": "boilerplate for angularjs 1.x component architecture build with webpack",
  "version": "1.0.0",
  "author": "web hollywood",
  "license": "MIT",
  "keywords": [
    "ES6",
    "webpack",
    "angularjs",
    "component",
    "class"
  ],
  "scripts": {
    "test": "mocha",
    "build": "cross-env NODE_ENV=production webpack -p",
    "build:dev": "cross-env NODE_ENV=dev webpack-dev-server",
    "build:prod": "npm run clean && npm run build",
    "clean": "rimraf build",
    "start": "npm run build:dev"
  },
  "devDependencies": {
    "angular": "^1.6.7",
    "autoprefixer": "^7.1.1",
    "babel-core": "^6.17.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-angularjs-annotate": "^0.7.0",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-1": "^6.16.0",
    "bootstrap": "^4.1.1",
    "chai": "^4.1.2",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.4",
    "eslint": "^4.12.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "mocha": "^5.2.0",
    "node-libs-browser": "2.0.0",
    "node-sass": "^4.7.2",
    "null-loader": "^0.1.1",
    "postcss-loader": "1.2.2",
    "puppeteer": "^1.5.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.1",
    "sass-loader": "^6.0.6",
    "sleep-promise": "^8.0.1",
    "style-loader": "^0.18.2",
    "tree-kill": "^1.2.0",
    "webpack": "2.2.0",
    "webpack-dev-server": "2.2.0"
  },
  "dependencies": {}
}
